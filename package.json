{
  "name": "sudoku-shuffle",
  "version": "1.0.0",
  "description": "Play Sudoku.",
  "contributors": [
    "Ben Storlie <benjstorlie@gmail.com> (https://benjstorlie.github.io/)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/benjstorlie/sudoku-shuffle.git"
  },
  "keywords": [
    "sudoku",
    "MERN",
    "game"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "develop": "concurrently \"npm run start-client\" \"npm run start-server\"",
    "start": "node server/server.js",
    "start-server": "cd server && npm start",
    "install": "cd server && npm i && cd ../client && npm i",
    "start-client": "cd client && npm start",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.1"
  }
}
